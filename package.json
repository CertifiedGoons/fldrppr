{
  "name": "fldrppr",
  "version": "1.0.0",
  "description": "Upload / Download files using http",
  "private": true,
  "scripts": {
    "start": "node bin/dev",
    "dev": "nodemon -r dotenv/config -e \"sass\" --exec \"yarn run sass && yarn run start\"",
    "sass": "node-sass src/sass -o public/css",
    "clean": "rm -rf dist",
    "build": "yarn run clean && mkdir dist && babel src -s -d dist && cp -r src/views dist/views && cp -r src/public dist/public",
    "production": "yarn run build && node bin/production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CertifiedGoons/fldrppr.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/CertifiedGoons/fldrppr/issues"
  },
  "homepage": "https://github.com/CertifiedGoons/fldrppr#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bower": "^1.8.4",
    "browser-sync": "^2.24.7",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "yarn": "^1.9.4"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.1",
    "dotenv": "^6.0.0",
    "dropzone": "^5.5.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "http-status-codes": "^1.3.0",
    "mongojs": "^2.6.0",
    "mongoose": "^5.2.14",
    "multer": "^1.3.1",
    "multer-gridfs-storage": "^3.2.2",
    "passport": "^0.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
